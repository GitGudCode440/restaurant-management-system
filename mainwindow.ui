        <property name="styleSheet">
         <string notr="true">* {
    border: none;
    background-color: transparent;
    background: none;
    padding: 0;
    margin: 0;
}

#centralwidget {
    background-color: #FAFAFA; /* Light gray for the main background */
}

#LeftMenuContainer {
    background-color: #F5F5F5; /* Slightly darker light gray for the sidebar */
}

QPushButton {
    background-color: #E8F0FE;  /* Light blue for buttons */
    color: #333333;            /* Dark gray text for readability */
    border: none;
    border-radius: 8px;        /* Rounded corners */
    padding: 12px 20px;        /* Balanced padding */
    font-size: 15px;           /* Medium font size for modern feel */
    font-weight: bold;         /* Emphasized text */
}

QPushButton:hover {
    background-color: #D7E6FD; /* Softer blue when hovered */
}

QPushButton:pressed {
    background-color: #C6D9FB; /* Slightly darker blue when pressed */
    color: #1A1A1A;            /* Darker text for contrast */
}

QPushButton:disabled {
    background-color: #F0F0F0; /* Muted gray for disabled buttons */
    color: #A8A8A8;            /* Muted text color */
}

QLineEdit {
    border: 2px solid green; /* Border color */
    border-radius: 5px; /* Rounded corners */
    padding: 5px; /* Internal padding for text */
